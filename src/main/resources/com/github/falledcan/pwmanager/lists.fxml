<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox id="listcontents" prefHeight="37.0" prefWidth="720.0" style="-fx-background-color: #DEDEDE;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.falledcan.pwmanager.ListsController">
   <children>
      <Label id="pwlist" fx:id="service" alignment="CENTER" prefHeight="37.0" prefWidth="277.0" stylesheets="@style.css" text="Google" />
      <Label id="pwlist" fx:id="username" alignment="CENTER" prefHeight="37.0" prefWidth="190.0" stylesheets="@style.css" text="Beerchan" />
      <Label id="pwlist" fx:id="password" alignment="CENTER" prefHeight="37.0" prefWidth="190.0" stylesheets="@style.css" text="********" />
      <Pane prefHeight="37.0" prefWidth="124.0">
         <children>
            <Button id="memo" fx:id="memo" contentDisplay="RIGHT" layoutX="14.0" layoutY="4.0" mnemonicParsing="false" stylesheets="@style.css">
               <font>
                  <Font size="6.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@memo.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="edit" fx:id="edit" layoutX="65.0" layoutY="4.0" mnemonicParsing="false" stylesheets="@style.css">
               <font>
                  <Font size="6.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@edit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </Pane>
   </children>
</HBox>
